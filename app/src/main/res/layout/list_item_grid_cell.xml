<?xml version="1.0" encoding="utf-8"?>
<com.example.sep490_mobile.ui.custom.BookingCellLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cell_container"
    android:layout_width="60dp"
    android:layout_height="match_parent"
    android:background="@drawable/cell_state_background"
    android:clickable="true"
    android:focusable="true">

    <ImageView
        android:id="@+id/iv_star_icon"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_gravity="center"
        android:src="@drawable/ic_star_yellow"
        android:visibility="gone"
        android:elevation="2dp"
        android:contentDescription="@string/booked_slot_indicator" />

</com.example.sep490_mobile.ui.custom.BookingCellLayout>